{
    "name": "zenas/php-test-generator",
    "description": "Generate tests for PHP classes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zenas",
            "email": "zilvinas.pranaitis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "myclabs/deep-copy": "^1.10",
        "nikic/php-parser": "^4.10",
        "phpdocumentor/reflection-docblock": "4.*",
        "symfony/config": "4.*",
        "symfony/console": "4.*",
        "symfony/dependency-injection": "4.*",
        "symfony/filesystem": "4.*",
        "symfony/property-info": "4.*",
        "symfony/yaml": "4.*"
    },
    "require-dev": {
        "clue/phar-composer": "^1.4",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Zenas\\PHPTestGenerator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/generate-php-test"
    ]
}
