{
    "name": "zenas/php-test-generator",
    "description": "Generate tests for PHP classes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zenas",
            "email": "zilvinas.pranaitis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "myclabs/deep-copy": "^1.10",
        "nikic/php-parser": "^4.10",
        "phpdocumentor/reflection-docblock": "^4.3|^5.3",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/filesystem": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/property-info": "^4.4",
        "symfony/yaml": "^4.4"
    },
    "require-dev": {
        "clue/phar-composer": "^1.4",
        "phpunit/phpunit": "^7|^8|^9",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Zenas\\PHPTestGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/generate-php-test"
    ]
}
