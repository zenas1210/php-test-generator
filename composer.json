{
    "name": "zenas/php-test-generator",
    "description": "Generate tests for PHP classes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zenas",
            "email": "zilvinas.pranaitis@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.3",
        "symfony/console": "^4.4|^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/filesystem": "^4.4|^5.3",
        "symfony/property-info": "^5.3",
        "symfony/string": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Zenas\\PHPTestGenerator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "bin": [
        "bin/generate-php-test"
    ]
}
